<%
out.println("<table>");
for (int i = 2; i < 10; i++) {
	out.println("<th>" + i + "단 </th>");
}
for (int i = 2; i < 10; i++) {
	out.print("<tr>");
	for (int j = 1; j < 10; j++) {
		out.println("<td>" + i + " X " + j + " = " + j * i + "</td>");
	}	
	out.print("</tr>");
}
out.println("</table>");
%>
<%
out.println("<table>");
for (int i = 2; i < 10; i += 3) {
	for (int j = 1; j < 10; j++) {
		for (int k = i; k < k+3; k++) {
			if (j == 1) {
				for (int l = k; l < l+3; l++) {
					out.println("<th>" + i + "단 </th>");
				}
			} else {
				out.println("<td>" + k + " X " + j + " = " + j * i + "</td>");
			}
		}
	}
}
out.println("</table>");
%>

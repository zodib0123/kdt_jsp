<%
out.println("<table>");
for (int i = 2; i < 10; i++) {
	out.println("<th>" + i + "단 </th>");
}
for (int i = 2; i < 10; i++) {
	out.print("<tr>");
	for (int j = 1; j < 10; j++) {
		out.println("<td>" + i + " X " + j + " = " + j * i + "</td>");
	}	
	out.print("</tr>");
}
out.println("</table>");
%>
<%
out.println("<table>");
for (int i = 2; i < 10; i += 3) {
	for (int j = 1; j < 10; j++) {
		if (j == 1) {
			for (int k = i; k < i+3; k++) {
				if (k < 10) {
					out.println("<th>" + k + "단 </th>");
				}
			}
		}
		out.print("<tr>");
		for (int k = i; k < i+3; k++) {
			if (k < 10) {
				out.println("<td>" + k + " X " + j + " = " + j * i + "</td>");
			}
		}
		out.print("</tr>\n");
	}
}
out.println("</table>");
%>
